                                @Html.TextBoxFor(model => Model.U_S10_P1, new { @class = "form-control", @type = "radio", @Value = "No", @autocomplete = "of" }) Si
                            </label>
                            <label class="btn btn-outline-secondary">
                                @Html.TextBoxFor(model => Model.U_S10_P1, new { @class = "form-control", @type = "radio", @Value = "No", @autocomplete = "of" }) No
                            </label>
                            <label class="btn btn-outline-secondary">
